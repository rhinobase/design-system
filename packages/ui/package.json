{
  "name": "@rafty/ui",
  "description": "React + A11y + Tailwind CSS Dynamic Component Library",
  "type": "module",
  "version": "2.0.0",
  "license": "MIT",
  "types": "./index.d.ts",
  "main": "./index.js",
  "scripts": {
    "build": "rollup -c=./rollup.config.js"
  },
  "keywords": [
    "react",
    "rafty-ui",
    "rafty",
    "design-system",
    "tailwind",
    "tailwindcss",
    "tailwind-css"
  ],
  "homepage": "https://rafty.rhinobase.io/",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhinobase/raftyui.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/rhinobase/raftyui/issues"
  },
  "dependencies": {
    "@ark-ui/react": "^3.3.0",
    "@heroicons/react": "^2.1.3",
    "@hookform/error-message": "^2.0.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@swc/helpers": "~0.5.11",
    "@tanstack/react-query": "^5.32.0",
    "class-variance-authority": "^0.7.0",
    "cmdk": "^0.2.0",
    "dateformat": "^5.0.3",
    "react-day-picker": "^8.8.2",
    "react-icons": "^5.2.1",
    "react-virtualized-auto-sizer": "^1.0.21",
    "react-window": "^1.8.10",
    "react-window-infinite-loader": "^1.0.9",
    "tailwind-merge": "^2.2.0"
  },
  "peerDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^18.2.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "require": {
        "types": "./index.d.cts",
        "default": "./index.cjs"
      }
    },
    "./accordion": {
      "import": {
        "types": "./accordion/index.d.ts",
        "default": "./accordion/index.js"
      },
      "require": {
        "types": "./accordion/index.d.cts",
        "default": "./accordion/index.cjs"
      }
    },
    "./alert": {
      "import": {
        "types": "./alert/index.d.ts",
        "default": "./alert/index.js"
      },
      "require": {
        "types": "./alert/index.d.cts",
        "default": "./alert/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@types/node": "20"
  }
}
